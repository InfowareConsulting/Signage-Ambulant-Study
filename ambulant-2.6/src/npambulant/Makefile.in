#
# These targets are magic for automake
#
all-am: build-npambulant

install-exec-hook: install-npambulant

@WITH_CG_TRUE@mostlyclean-local:
@WITH_CG_TRUE@	rm -fr .libs *o $(NPAMBULANT) $(NPAMBULANT_PLUGIN)

@WITH_CG_TRUE@uninstall-local:
@WITH_CG_TRUE@	-rm -rf $(MOZILLA_PLUGINS)/$(NPAMBULANT_PLUGIN)
@WITH_CG_FALSE@mostlyclean-local:
@WITH_CG_FALSE@	-rm -fr .libs *o $(NPAMBULANT) npambulant_plugin_dir
@WITH_CG_FALSE@	-rm -fr mozilla npambulant

@WITH_CG_FALSE@uninstall-local:
@WITH_CG_FALSE@	if [ `id -u` == "0" ]; then MOZILLA_PLUGINS=/usr/lib/$(MOZILLA_PLUGINS); else MOZILLA_PLUGINS=~/.$(MOZILLA_PLUGINS);fi; \
@WITH_CG_FALSE@	rm -fr $$MOZILLA_PLUGINS/npambulant.so $$MOZILLA_PLUGINS/npambulant;

#
# To build the shared library we want we should build the libtool library.
# You know this, and I know this, but "make" doesn't, so we tell it here:
#
.libs/$(NPAMBULANT): $(lib_LTLIBRARIES)
check-npambulant:
	if test x$$DISPLAY = x; then \
		echo Skipping tests, DISPLAY not set; \
	else \
		a=`cd $(ambulantlibdir); pwd` ; \
		cd $(srcdir) ; \
		LD_LIBRARY_PATH=-L"$$a" $(FIREFOXENV) FIREFOXPATH=$(BUILDLIBDIR) $(DEBUGGER) $(FIREFOX) $(FIREFOXARGS) `pwd`/test/test_npambulant.html ; \
	fi

testplay:
	cd $(srcdir) ; $(FIREFOXENV) FIREFOXPATH=$(BUILDLIBDIR) $(DEBUGGER) $(FIREFOX) $(FIREFOXARGS) test/test_npambulant.html

#------------------------------------------------------
#
# Create the structure that will eventually end up in the Firefox
# plugins directory.
#
@WITH_CG_TRUE@build-npambulant: $(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@$(NPAMBULANT_PLUGIN): .libs/$(NPAMBULANT) $(srcdir)/Info.plist
@WITH_CG_TRUE@	-rm -fr $(NPAMBULANT_PLUGIN)

#	Install boilerplate, Info.plist, etc.
@WITH_CG_TRUE@	mkdir $(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@	mkdir $(NPAMBULANT_PLUGIN)/Contents
@WITH_CG_TRUE@	cp $(srcdir)/Info.plist $(NPAMBULANT_PLUGIN)/Contents

#	Install npambulant plugin binary
@WITH_CG_TRUE@	-mkdir $(NPAMBULANT_PLUGIN)/Contents/MacOS
@WITH_CG_TRUE@	cp .libs/$(NPAMBULANT) $(NPAMBULANT_PLUGIN)/Contents/MacOS/$(NPAMBULANT)

#	Install ambulant core libraries

#	Install plugins for ambulant engine
@WITH_CG_TRUE@	-mkdir $(NPAMBULANT_PLUGIN)/Contents/PlugIns
@WITH_CG_TRUE@	pkglibdir=`pwd`/$(NPAMBULANT_PLUGIN)/Contents/PlugIns; \
@WITH_CG_TRUE@	cd ../plugins; \
@WITH_CG_TRUE@	$(MAKE) $(AM_MAKEFLAGS) pkglibdir=$$pkglibdir install 

#	Install gettext localizations
@WITH_CG_TRUE@	localedir=`pwd`/$(BUILDAPPNAME).app/Contents/Resources/locale; \
@WITH_CG_TRUE@	cd ../../po ; \
@WITH_CG_TRUE@	$(MAKE) $(AM_MAKEFLAGS) localedir=$$localedir install

@WITH_CG_TRUE@install-npambulant: $(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@	-rm -fr $(MOZILLA_PLUGINS)/$(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@	cp -R $(NPAMBULANT_PLUGIN) $(MOZILLA_PLUGINS)/$(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@	-mkdir $(NPAMBULANT_PLUGIN)/Contents/MacOS
@WITH_CG_TRUE@	cp .libs/$(NPAMBULANT) $(NPAMBULANT_PLUGIN)/Contents/MacOS/$(NPAMBULANT)

#	Install ambulant core libraries

#	Install plugins for ambulant engine
@WITH_CG_TRUE@	-mkdir $(NPAMBULANT_PLUGIN)/Contents/PlugIns
@WITH_CG_TRUE@	pkglibdir=`pwd`/$(NPAMBULANT_PLUGIN)/Contents/PlugIns; \
@WITH_CG_TRUE@	cd ../plugins; \
@WITH_CG_TRUE@	$(MAKE) $(AM_MAKEFLAGS) pkglibdir=$$pkglibdir install 

#	Copy in shared libraries and fix rpath
@WITH_CG_TRUE@	DYLD_LIBRARY_PATH=$(top_builddir)/src/libambulant/.libs:$(top_builddir)/third_party_packages/installed/lib \
@WITH_CG_TRUE@		$(PYTHON) $(top_srcdir)/scripts/internalize_dylibs.py -v\
@WITH_CG_TRUE@		$(NPAMBULANT_PLUGIN)
#	Check that internalization worked
@WITH_CG_TRUE@	$(PYTHON) $(top_srcdir)/scripts/internalize_dylibs.py -c $(NPAMBULANT_PLUGIN)
@WITH_CG_TRUE@	strip  npambulant_plugin_dir/plugins/npambulant/*.so*

#	Install gettext localizations
@WITH_CG_TRUE@	localedir=`pwd`/$(BUILDAPPNAME).app/Contents/Resources/locale; \
@WITH_CG_TRUE@	cd ../../po ; \
@WITH_CG_TRUE@	$(MAKE) $(AM_MAKEFLAGS) localedir=$$localedir install

@WITH_CG_FALSE@build-npambulant: npambulant_plugin_dir
@WITH_CG_FALSE@npambulant_plugin_dir: .libs/$(NPAMBULANT)
#	Create directory structure
@WITH_CG_FALSE@	-rm -fr npambulant_plugin_dir
@WITH_CG_FALSE@	mkdir npambulant_plugin_dir
@WITH_CG_FALSE@	mkdir npambulant_plugin_dir/plugins
@WITH_CG_FALSE@	mkdir npambulant_plugin_dir/plugins/npambulant
@WITH_CG_FALSE@	mkdir npambulant_plugin_dir/plugins/npambulant/plugins

#	Copy browser plugin
@WITH_CG_FALSE@	cp .libs/$(NPAMBULANT) npambulant_plugin_dir/plugins/npambulant.so
@WITH_CG_FALSE@	strip npambulant_plugin_dir/plugins/npambulant.so

#	Install plugins for ambulant engine
@WITH_CG_FALSE@		pkglibdir=`pwd`/npambulant_plugin_dir/plugins/npambulant/plugins; \
@WITH_CG_FALSE@		cd ../plugins; \
@WITH_CG_FALSE@		cp .libs/*.{so.0,la} $$pkglibdir

#	Copy in shared libraries and fix rpath
#	Ubuntu 11.04 32/64 bit
@WITH_CG_FALSE@	LD_LIBRARY_PATH=$(top_builddir)/src/libambulant/.libs:$(top_builddir)/third_party_packages/installed/lib \
@WITH_CG_FALSE@	$(PYTHON) $(top_srcdir)/scripts/internalize_shlibs.py \
@WITH_CG_FALSE@		npambulant_plugin_dir/plugins
#	Check that internalization worked
@WITH_CG_FALSE@	$(PYTHON) $(top_srcdir)/scripts/internalize_shlibs.py -c npambulant_plugin_dir/plugins
#	Copy manifest file (only used for xpt distributions)
@WITH_CG_FALSE@	@case $(ARCH) in		    \
@WITH_CG_FALSE@		x86_64|X86_64)      \
@WITH_CG_FALSE@		 	cp $(srcdir)/install-linux64.rdf npambulant_plugin_dir/install.rdf ;; \
@WITH_CG_FALSE@		i386|i686)			\
@WITH_CG_FALSE@			cp $(srcdir)/install-linux.rdf npambulant_plugin_dir/install.rdf ;;	\
@WITH_CG_FALSE@	esac
@WITH_CG_FALSE@	cp $(srcdir)/manifest.json npambulant_plugin_dir/manifest.json
@WITH_CG_FALSE@	strip  npambulant_plugin_dir/plugins/npambulant/*.so*

@WITH_CG_FALSE@install-npambulant:  npambulant_plugin_dir
@WITH_CG_FALSE@	if [ `id -u` == "0" ]; then \
@WITH_CG_FALSE@		MOZILLA_PLUGINS=/usr/lib/$(MOZILLA_PLUGINS); \
@WITH_CG_FALSE@	else \
@WITH_CG_FALSE@		MOZILLA_PLUGINS=~/.$(MOZILLA_PLUGINS);\
@WITH_CG_FALSE@	fi; \
@WITH_CG_FALSE@	$(INSTALL) -d $(MOZILLA_PLUGINS);	\
@WITH_CG_FALSE@	$(INSTALL_PROGRAM) npambulant_plugin_dir/plugins/npambulant.so $$MOZILLA_PLUGINS/npambulant.so;	\
@WITH_CG_FALSE@	(cd  npambulant_plugin_dir/plugins ; tar cf - npambulant  ) | \
@WITH_CG_FALSE@		(cd $$MOZILLA_PLUGINS ; tar xf -)
#------------------------------------------------------
#
# Build the .xpi file which can be fed to Firefox to install the plugin
# and when Google Chrome browser is installed, the .crx for that browser as well
#
installer: $(INSTALLER_ARCHIVE)

$(INSTALLER_ARCHIVE): npambulant_plugin_dir
	-rm -f $(INSTALLER_ARCHIVE)
	-rm -f npambulant-$(VERSION)-linux-$(ARCH).crx
	cd npambulant_plugin_dir; \
		zip -r ../$(INSTALLER_ARCHIVE) *

signed_installer: installer
	codesign -s "Developer ID Application" -f --deep -v $(INSTALLER_ARCHIVE)
#	codesign -s "Stichting CWI" -f --deep -v $(INSTALLER_ARCHIVE)

npambulant-$(VERSION)-linux-$(ARCH).crx: npambulant_plugin_dir
	which google-chrome; \
	if test $$?; then cd ..; \
		rm -f  npambulant_plugin_dir.pem ; \
		Xvfb :829 & DISPLAY=:829 google-chrome --enable-apps --pack-extension=$$PWD/npambulant_plugin_dir; killall Xvfb; \
	fi
#	-rm -f /tmp/.X829
	-mv -f npambulant_plugin_dir.crx npambulant-$(VERSION)-linux-$(ARCH).crx

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
